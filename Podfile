# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'
use_frameworks!

target 'AugmentisTrip' do

  # Firebase SDK
  pod 'Firebase'
  pod 'Firebase/Database’
  pod 'Firebase/Auth’
  pod 'Firebase/Storage’
  pod 'Firebase/Core'

  # Google Places API
  pod 'GooglePlaces'
  pod 'GooglePlacePicker'
  pod 'GoogleMaps'
  
  # JSQMessages
  pod 'JSQMessagesViewController'

end
